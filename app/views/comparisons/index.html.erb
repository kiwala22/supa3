<%= provide(:title, "Draw Comparisons") %>
<h3>
	<%= "Draw Comparisons" %>
<small><%= "All Draws" %></small>
</h3>


<div>
  <table class="table table-striped table-bordered table-hoverlarge-12 columns large-centered">
      <thead>
        <tr>
          <th>Date</th>
          <th>Tickets</th>
          <th>Users</th>
					<th>RTP Change</th>
          <th>Ticket Change</th>
        </tr>
      </thead>
      <tbody>
				<% @series.each do |f| %>
            <tr>
              <td><%= f[:date] %></td>
							<td><%= f[:tickets] %></td>
							<td><%= f[:users] %></td>
							<td><%= number_to_percentage(f[:rtp_change], precision: 2) %></td>
							<td><%= f[:ticket_change] %>
            </tr>
				<% end %>
      </tbody>
  </table>
</div>
