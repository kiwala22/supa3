$(document).ready(() => {
    // require 'walden'
    try{var myChart = echarts.init(document.getElementById('revenue_analytics'), 'walden');}catch(e){};
    var option = {
    title: {
        text: 'Revenue Analytics',
        subtext: 'Distribution of Revenues'
        },
        tooltip : {
            trigger: 'axis',
            axisPointer : {            // 坐标轴指示器，坐标轴触发有效
                type : 'shadow'        // 默认为直线，可选为：'line' | 'shadow'
            }
        },
        legend: {
            data:['Revenues', 'Payouts']
        },
        grid: {
            left: '5%',
            right: '5%',
            bottom: '5%',
            containLabel: true
        },
        xAxis : [
            {
                type : 'category',
                data : gon.labels
            }
        ],
        yAxis : [
            {
                type : 'value'
            }
        ],
        series : [
            {
                name:'Revenues',
                type:'bar',
                barWidth : 12,
                itemStyle: {color: '#91c7ae'},
                data: gon.revenues
            },
            {
                name:'Payouts',
                type:'bar',
                barWidth : 12,
                itemStyle: {color: '#61a0a8'},
                data: gon.payouts
            }
        ]
    };

    // use configuration item and data specified to show chart
    try{myChart.setOption(option);}catch(e){};

});